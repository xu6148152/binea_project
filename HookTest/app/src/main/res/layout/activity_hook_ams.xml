<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context="com.example.binea.hooktest.ams_hook.HookAMSActivity">

    <Button android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="ams hook"
            android:onClick="onTestAMS"
            android:id="@+id/btn_ams"/>

    <Button android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="pms hook"
            android:onClick="onTestPMS"
            app:layout_constraintTop_toBottomOf="@id/btn_ams"/>
</android.support.constraint.ConstraintLayout>
